{
    ajv: {
        removeAdditional: true,
        useDefaults: true,
        coerceTypes: true
    },
    redis: {
        port: 6379,
        ttlInSeconds: 90
    },
    fastify: {
        ignoreTrailingSlash: true,
        logger: true
    },
    app: {
        maxGuardsCount: 3,
        guardExpirationInSeconds: 60
    },
    cors: {
        methods: ["POST", "PATCH", "DELETE"],
        origin: "*",
        maxAge: 86400,
        allowedHeaders: ["content-type", "x-user-id"],
        strictPreflight: false
    }
}